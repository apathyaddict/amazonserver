{
    "version": 2,
     "builds": [
        { "src": "server.js", "use": "@vercel/node" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "access-control-allow-origin",
            "value": "*"
          }
        ]
      }
    ],
    "rewrites": [
      {
          "source": "/(.*)",
          "destination": "/"
      }
  ]
  }
  